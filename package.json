    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3"
  },
  "name": "documenso.next",
  "packageManager": "npm@8.19.2",
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/email/.react-email"
  ]
}
